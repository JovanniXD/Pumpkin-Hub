local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("Toffus Hub test", "DarkTheme")

local Tab = Window:NewTab("Main")

local Section = Tab:NewSection("Main Things")

Section:NewToggle("Disable Egg Animation", "yes", function(state)

    if state then

        game.ReplicatedStorage.Assets.UI.EggFrame.Visible = false

    else

        game.ReplicatedStorage.Assets.UI.EggFrame.Visible = true

    end

end)

Section:NewButton("Auto Single 80M Egg (read info by clicking 3 dots)", "im too lazy to make a toggleable button so ill make it tmrw you cna only stop this by rejoin", function()

    while true do

        wait(0.)

local args = {

    [1] = {},

    [2] = "80M Egg",

    [3] = 1

}

game:GetService("ReplicatedStorage").Events.HatchEgg:InvokeServer(unpack(args))

end

end)

Section:NewButton("Auto Click (read info by clicking 3 dots)", "im too lazy to make a toggleable button so ill make it tmrw you cna only stop this by rejoin", function()

    while true do

        wait(0.)

-- Script generated by Jovanni - credits to Bani#9034

local args = {

    [1] = "Main"

}

game:GetService("ReplicatedStorage").Events.Tap:FireServer(unpack(args))

end

end)
